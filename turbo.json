{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@acme/api#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "generate": {
      "outputs": []
    },
    "migrate:dev": {
      "dependsOn": ["^generate"],
      "env": ["DATABASE_URL"],
      "outputs": []
    },
    "seed": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "env:local": {
      "outputs": []
    },
    "storybook": {
      "outputs": []
    }
  }
}
